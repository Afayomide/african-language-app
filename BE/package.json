    "@aws-sdk/client-s3": "^3.670.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.6",
    "eslint": "^9.9.0",
